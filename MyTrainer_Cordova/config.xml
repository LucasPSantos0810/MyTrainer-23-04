<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.mytrainer" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>MyTrainer</name>
    <description>
        Aplicativo de encontro e contrato de servi√ßos de Personal Trainer.
    </description>
    <author email="mytraineretec@gmail.com" href="http://cordova.io">
        5DEVERS
    </author>
    <content src="preindex.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
		<icon src="www/img/logo.ico" platform="android" width="57" height="57" density="mdpi" />
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
		<icon src="www/img/logo.ico" platform="ios" width="57" height="57" density="mdpi" />
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
